{
  "name": "cmdb.js",
  "description": "A javascript library for interacting with the CMDB v3",
  "version": "3.0.4",
  "main": "dist/cmdb.js",
  "browser": "dist/cmdb.browser.js",
  "module": "dist/cmdb.mjs",
  "scripts": {
    "clean": "del ./dist",
    "test": "jest __tests__/src ",
    "test:coverage": "npm test -- --coverage",
    "test:dist": "jest __tests__/dist",
    "build:browser": "rollup -c rollup.config.browser.js",
    "build:esmodule": "rollup -c rollup.config.module.js",
    "build:node": "rollup -c rollup.config.node.js",
    "build": "npm-run-all clean --parallel build:**",
    "prepublishOnly": "npm run build",
    "lint": "eslint --cache --fix .",
    "precommit": "lint-staged",
    "prepush": "npm-run-all test build test:dist",
    "postcommit": "git diff --name-only HEAD^ | xargs git add"
  },
  "engines": {
    "node": ">= 4.3.2"
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --cache --fix",
      "git add"
    ]
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      ".eslintrc"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Financial-Times/cmdb.js.git"
  },
  "keywords": [
    "FT",
    "CMDB"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Financial-Times/cmdb.js/issues"
  },
  "dependencies": {
    "isomorphic-unfetch": "^2.0.0"
  },
  "homepage": "https://github.com/Financial-Times/cmdb.js#readme",
  "devDependencies": {
    "@financial-times/eslint-config-de-tooling": "^2.0.2",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "del-cli": "^1.1.0",
    "eslint": "^4.8.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.4.2",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-prettier": "^2.3.1",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^6.0.0",
    "nock": "^9.1.4",
    "npm-run-all": "^4.1.1",
    "prettier": "^1.7.0",
    "puppeteer": "^0.13.0",
    "rollup": "^0.52.1",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0"
  }
}
