{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc"
        ]
    },
    "source": {
        "include": [
            "index.js",
            "src/",
            "README.md"
        ],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs|__tests__|rollup.*|out|dist)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "useLongnameInNav": false,
        "showInheritedInNav": true
    },
    "opts": {
        "destination": "./docs/",
        "encoding": "utf8",
        "private": false,
        "recurse": true,
        "template": "./node_modules/minami"
    }
}
